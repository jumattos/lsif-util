    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Graph",
            "program": "${workspaceFolder}/lib/graph.js",
            "outFiles": [
				"${workspaceFolder}/lib/**/*.js"
            ],
            "sourceMaps": true,
            "args": [
                "5"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Search",
            "program": "${workspaceFolder}/lib/search.js",
            "outFiles": [
				"${workspaceFolder}/lib/**/*.js"
            ],
            "sourceMaps": true,
            "args": [
                "-id", "20", "16", "-inV", "15"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Validate",
            "program": "${workspaceFolder}/lib/validate.js",
            "outFiles": [
				"${workspaceFolder}/lib/**/*.js"
            ],
            "sourceMaps": true,
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Metadata",
            "program": "${workspaceFolder}/lib/metadata.js",
            "outFiles": [
				"${workspaceFolder}/lib/**/*.js"
            ],
            "sourceMaps": true,
            "args": []
        }
    ]
}